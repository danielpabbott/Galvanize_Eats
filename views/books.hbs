<div class='row books'>
  {{#each book}}
    <div class='col m3'>
      <div class="card">
        <div class="card-image">
          <img src="{{this.cover_url}}">
          <span class="card-title"></span>
        </div>
        <div class="card-action center">
          <a href="#">More Info</a>
        </div>
      </div>
    </div>
  {{/each}}
</div>
